<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zerovoid.zerovoidframe">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.zerovoid.application.InitApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.zerovoid.main.activity.MainActivity"
            android:label="@string/app_name">

        </activity>
        <activity android:name="com.zerovoid.test.TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zerovoid.house.activity.HouseListActivityWithLv"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.zerovoid.screen.ScreenAdapterActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>
